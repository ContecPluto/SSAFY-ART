A,MAAM,CAACgB,YAAP;AACD;;AACD,MAAIT,EAAE,CAACU,IAAH,CAAQC,YAAZ,EAA0B;AACxB,QAAIJ,OAAJ,EAAa;AACXD,MAAAA,IAAI;AACL,KAFD,MAEO,IAAI,CAACC,OAAL,EAAc;AACnBK,MAAAA,KAAK,CAAC,aAAD,CAAL;AACAf,MAAAA,MAAM,CAACgB,IAAP,CAAY;AAACC,QAAAA,IAAI,EAAC;AAAN,OAAZ;AACD;AACF;;AAEDR,EAAAA,IAAI;AACL,CAlBD;AAoBA,IAAIpB,GAAJ,CAAQ;AACNW,EAAAA,MAAM,EAANA,MADM;AAENL,EAAAA,KAAK,EAALA,KAFM;AAGNuB,EAAAA,MAAM,EAAE,gBAAAC,CAAC;AAAA,WAAIA,CAAC,CAAC7B,GAAD,CAAL;AAAA;AAHH,CAAR,EAIG8B,MAJH,CAIU,MAJV","sourcesContent":["import Vue from 'vue'\nimport App from './App.vue'\nimport Router from 'vue-router'\nimport routes from './routes'\nimport { BootstrapVue, IconsPlugin } from 'bootstrap-vue'\nimport store from './store'\nimport cookie from './js/cookie'\n// import cookie from '@/js/cookie.js'\n\nVue.config.productionTip = false\n// Install BootstrapVue\nVue.use(BootstrapVue)\n// Optionally install the BootstrapVue icon components plugin\nVue.use(IconsPlugin)\nVue.use(Router)\n\nconst router = new Router({\n  mode: 'history',\n  routes,\n  scrollBehavior (to, from, savedPosition) {\n    if (savedPosition) {\n      return savedPosition\n    } else {\n      return { x: 0, y: 0 }\n    }\n  }\n});\n\n\nrouter.beforeEach((to, from, next) => {\n  var isLogin = false\n  if (cookie.token()) {\n    isLogin = true\n  } \n  if (isLogin) {\n    cookie.updateCookie()\n  }\n  if (to.meta.loginRequire) {\n    if (isLogin) {\n      next()\n    } else if (!isLogin) {\n      alert('로그인이 필요합니다.')\n      router.push({name:'Main'})\n    }\n  }\n\n  next()\n})\n\nnew Vue({\n  router,\n  store,\n  render: h => h(App),\n}).$mount('#app')\n"]}]}